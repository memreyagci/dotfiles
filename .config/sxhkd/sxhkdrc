super + shift + Return
    $TERMINAL

super + shift + q
    power-management-options

super + w
    $BROWSER

super + d
    dmenu_run

super + e
    /home/musa/.local/bin/cronjobs/cj-mail; st -e neomutt; /home/musa/.local/bin/cronjobs/cj-mail

super + x
    slock -m "Locked at $(date +'%I:%M %p')"

Print
    flameshot screen -c -p $HOME/Pictures/screenshots

super + Print
    flameshot gui

XF86Audio{Mute,MicMute,RaiseVolume,LowerVolume}
    pactl{ set-sink-mute @DEFAULT_SINK@ toggle; pkill -RTMIN+10 i3blocks, set-source-mute @DEFAULT_SOURCE@ toggle; pkill -RTMIN+10 i3blocks,-increase; pkill -RTMIN+10 i3blocks, set-sink-volume @DEFAULT_SINK@ -5%; pkill -RTMIN+10 i3blocks}

XF86MonBrightness{Up,Down}
    xbacklight {-inc 5; pkill -RTMIN+5 i3blocks,-dec 5; pkill -RTMIN+5 i3blocks}

super + f
    st -e lfrun

super + o; {b,s,m,t,e}
    {bitwarden-desktop,signal-desktop,teams,telegram-desktop,thunderbird}

super + r; {d,s}
    {pkill -9 dwmblocks; dwmblocks, pkill -9 sxhkd; sxhkd}
